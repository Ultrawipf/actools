            <c:SpacingUniformGrid x:Name="Ring" Margin="{TemplateBinding Padding}" Visibility="Collapsed" Columns="2" Rows="2" HorizontalSpacing="4"
                VerticalSpacing="4" RenderTransformOrigin="0.5,0.5" Width="54" Height="54" IsHitTestVisible="False">
