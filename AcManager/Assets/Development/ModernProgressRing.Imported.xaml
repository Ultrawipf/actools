                      <EasingDoubleKeyFrame KeyTime="0:0:0.34" Value="0.5">
                        <EasingDoubleKeyFrame.EasingFunction>
                          <BounceEase Bounces="2" Bounciness="3.5" />
                        </EasingDoubleKeyFrame.EasingFunction>
                      </EasingDoubleKeyFrame>
                      <DiscreteDoubleKeyFrame KeyTime="0:0:0.6" Value="0.5" />
