                      <DiscreteDoubleKeyFrame KeyTime="0:0:0.22" Value="1" />
                      <EasingDoubleKeyFrame KeyTime="0:0:0.36" Value="0.5">
                        <EasingDoubleKeyFrame.EasingFunction>
                          <BounceEase Bounces="2" Bounciness="3.5" />
