  <Style x:Key="_Dev" TargetType="c:ModernProgressRing">
    <Setter Property="Background" Value="Transparent" />
    <Setter Property="Foreground" Value="{DynamicResource Accent}" />
    <Setter Property="IsHitTestVisible" Value="False" />
    <Setter Property="HorizontalAlignment" Value="Center" />
    <Setter Property="VerticalAlignment" Value="Center" />
    <Setter Property="MinHeight" Value="32" />
    <Setter Property="MinWidth" Value="32" />
    <Setter Property="IsTabStop" Value="False" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="c:ModernProgressRing">
          <Border Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="SizeStates">
                <VisualState x:Name="Large" />
                <VisualState x:Name="Small" />
              </VisualStateGroup>
              <VisualStateGroup x:Name="ActiveStates">
                <VisualState x:Name="Inactive" />
                <VisualState x:Name="Active">
                  <Storyboard RepeatBehavior="Forever">
                    <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="Ring" Storyboard.TargetProperty="Visibility">
                      <DiscreteObjectKeyFrame KeyTime="0" Value="{x:Static Visibility.Visible}" />
                    </ObjectAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.Opacity)"
                        Storyboard.TargetName="ProgressElement1">
                      <EasingDoubleKeyFrame KeyTime="0" Value="1" />
                      <SplineDoubleKeyFrame KeyTime="0:0:1" Value="0" KeySpline="0.4,0 0.6,1" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.RenderTransform).(ScaleTransform.ScaleX)"
                        Storyboard.TargetName="ProgressElement1">
                      <EasingDoubleKeyFrame KeyTime="0" Value="0" />
                      <SplineDoubleKeyFrame KeyTime="0:0:1" Value="1" KeySpline="0.4,0 0.2,1" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.RenderTransform).(ScaleTransform.ScaleY)"
                        Storyboard.TargetName="ProgressElement1">
                      <EasingDoubleKeyFrame KeyTime="0" Value="0" />
                      <SplineDoubleKeyFrame KeyTime="0:0:1" Value="1" KeySpline="0.4,0 0.2,1" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.Opacity)"
                        Storyboard.TargetName="ProgressElement2" BeginTime="-0:0:0.2">
                      <EasingDoubleKeyFrame KeyTime="0" Value="1" />
                      <SplineDoubleKeyFrame KeyTime="0:0:1" Value="0" KeySpline="0.4,0 0.6,1" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.RenderTransform).(ScaleTransform.ScaleX)"
                        Storyboard.TargetName="ProgressElement2" BeginTime="-0:0:0.2">
                      <EasingDoubleKeyFrame KeyTime="0" Value="0" />
                      <SplineDoubleKeyFrame KeyTime="0:0:1" Value="1" KeySpline="0.4,0 0.2,1" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.RenderTransform).(ScaleTransform.ScaleY)"
                        Storyboard.TargetName="ProgressElement2" BeginTime="-0:0:0.2">
                      <EasingDoubleKeyFrame KeyTime="0" Value="0" />
                      <SplineDoubleKeyFrame KeyTime="0:0:1" Value="1" KeySpline="0.4,0 0.2,1" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.Opacity)"
                        Storyboard.TargetName="ProgressElement3" BeginTime="-0:0:0.4">
                      <EasingDoubleKeyFrame KeyTime="0" Value="1" />
                      <SplineDoubleKeyFrame KeyTime="0:0:1" Value="0" KeySpline="0.4,0 0.6,1" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.RenderTransform).(ScaleTransform.ScaleX)"
                        Storyboard.TargetName="ProgressElement3" BeginTime="-0:0:0.4">
                      <EasingDoubleKeyFrame KeyTime="0" Value="0" />
                      <SplineDoubleKeyFrame KeyTime="0:0:1" Value="1" KeySpline="0.4,0 0.2,1" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.RenderTransform).(ScaleTransform.ScaleY)"
                        Storyboard.TargetName="ProgressElement3" BeginTime="-0:0:0.4">
                      <EasingDoubleKeyFrame KeyTime="0" Value="0" />
                      <SplineDoubleKeyFrame KeyTime="0:0:1" Value="1" KeySpline="0.4,0 0.2,1" />
                    </DoubleAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="Ring" Margin="{TemplateBinding Padding}" Visibility="Collapsed">
              <Ellipse x:Name="ProgressElement1" Stroke="{TemplateBinding Foreground}" StrokeThickness="2" RenderTransformOrigin="0.5,0.5">
                <Ellipse.RenderTransform>
                  <ScaleTransform />
                </Ellipse.RenderTransform>
              </Ellipse>
              <Ellipse x:Name="ProgressElement2" Stroke="{TemplateBinding Foreground}" StrokeThickness="4" RenderTransformOrigin="0.5,0.5">
                <Ellipse.RenderTransform>
                  <ScaleTransform />
                </Ellipse.RenderTransform>
              </Ellipse>
              <Ellipse x:Name="ProgressElement3" Stroke="{TemplateBinding Foreground}" StrokeThickness="6" RenderTransformOrigin="0.5,0.5">
                <Ellipse.RenderTransform>
                  <ScaleTransform />
                </Ellipse.RenderTransform>
              </Ellipse>
            </Grid>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="AzureProgressRingStyle" TargetType="c:ModernProgressRing">
    <Setter Property="Background" Value="Transparent" />
    <Setter Property="Foreground" Value="{DynamicResource Accent}" />
    <Setter Property="IsHitTestVisible" Value="False" />
    <Setter Property="HorizontalAlignment" Value="Center" />
    <Setter Property="VerticalAlignment" Value="Center" />
    <Setter Property="Height" Value="80" />
    <Setter Property="Width" Value="80" />
    <Setter Property="IsTabStop" Value="False" />
    <Setter Property="DensityMultiplier" Value="0.8" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="c:ModernProgressRing">
          <Border x:Name="PART_Border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}"
              Background="{TemplateBinding Background}">
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="SizeStates">
                <VisualState x:Name="Large" />
                <VisualState x:Name="Small" />
              </VisualStateGroup>
              <VisualStateGroup x:Name="ActiveStates">
                <VisualState x:Name="Inactive" />
                <VisualState x:Name="Active">
                  <Storyboard SpeedRatio="1" RepeatBehavior="Forever">
                    <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Path2" Storyboard.TargetProperty="StrokeDashOffset">
                      <LinearDoubleKeyFrame KeyTime="0:0:0" Value="25.2" />
                      <LinearDoubleKeyFrame KeyTime="0:0:.35" Value="0" />
                      <DiscreteDoubleKeyFrame KeyTime="0:0:.675" Value="0" />
                      <LinearDoubleKeyFrame KeyTime="0:0:1.025" Value="-25.2" />
                      <DiscreteDoubleKeyFrame KeyTime="0:0:1.4" Value="-25.2" />
                      <LinearDoubleKeyFrame KeyTime="0:0:1.75" Value="-50.4" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Path3" Storyboard.TargetProperty="StrokeDashOffset">
                      <DiscreteDoubleKeyFrame KeyTime="0:0:.15" Value="41" />
                      <LinearDoubleKeyFrame KeyTime="0:0:1.5" Value="-41" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Path10" Storyboard.TargetProperty="StrokeDashOffset">
                      <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="19.8" />
                      <LinearDoubleKeyFrame KeyTime="0:0:.25" Value="0" />
                      <DiscreteDoubleKeyFrame KeyTime="0:0:.6" Value="0" />
                      <LinearDoubleKeyFrame KeyTime="0:0:.975" Value="-29.8" />
                      <DiscreteDoubleKeyFrame KeyTime="0:0:1.6" Value="29.8" />
                      <LinearDoubleKeyFrame KeyTime="0:0:1.75" Value="19.8" />
                    </DoubleAnimationUsingKeyFrames>
                    <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Path11" Storyboard.TargetProperty="StrokeDashOffset">
                      <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="-10" />
                      <LinearDoubleKeyFrame KeyTime="0:0:.175" Value="-29.8" />
                      <DiscreteDoubleKeyFrame KeyTime="0:0:.85" Value="29.8" />
                      <LinearDoubleKeyFrame KeyTime="0:0:.975" Value="20.8" />
                      <LinearDoubleKeyFrame KeyTime="0:0:1.2635" Value="0" />
                      <DiscreteDoubleKeyFrame KeyTime="0:0:1.65" Value="0" />
                      <LinearDoubleKeyFrame KeyTime="0:0:1.75" Value="-10" />
                    </DoubleAnimationUsingKeyFrames>
                  </Storyboard>
                </VisualState>
              </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <c:Cell Background="Transparent">
              <Path x:Name="Path10" Stroke="{TemplateBinding Foreground}" StrokeThickness="4" Data="F1 M0,-10 H 0.000001 M20,0 A 10,10 180 1 1 0,0"
                  Stretch="Uniform" Width="80" Height="80" StrokeDashOffset="19.8" StrokeDashArray="29.8 1000" />
              <Path x:Name="Path11" Stroke="{TemplateBinding Foreground}" StrokeThickness="4" Data="F1 M0,10 H 0.000001 M0,0 A 10,10 180 1 1 20,0"
                  Stretch="Uniform" Width="80" Height="80" StrokeDashOffset="29.8" StrokeDashArray="29.8 1000" />
              <Path x:Name="Path2" Stroke="{TemplateBinding Foreground}" StrokeThickness="4" Data="F1 M20,0 A 10,10 180 1 1 0,0 A 10,10 180 1 1 20,0"
                  Stretch="Uniform" Width="68" Height="68" StrokeDashArray="25.2 1000" />
              <Path x:Name="Path3" Stroke="{TemplateBinding Foreground}" StrokeThickness="4" Data="F1 M20,0 A 10,10 180 1 1 0,0 A 10,10 180 1 1 20,0"
                  StrokeDashOffset="41" StrokeDashArray="41 1000" Stretch="Uniform" Width="56" Height="56" />
            </c:Cell>
          </Border>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
